<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane maxHeight="600.0" maxWidth="400.0" minHeight="500.0" minWidth="400.0" prefHeight="600.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.view.RegisterController">
    <children>
        <TextField fx:id="textFieldNick" alignment="CENTER" layoutX="52.0" layoutY="125.0" prefHeight="54.0" prefWidth="297.0" promptText="NICK" AnchorPane.bottomAnchor="422.0" />
        <TextField fx:id="textFieldLogin" alignment="CENTER" layoutX="52.0" layoutY="197.0" prefHeight="54.0" prefWidth="297.0" promptText="LOGIN" />
        <PasswordField fx:id="textFieldPassword" alignment="CENTER" layoutX="52.0" layoutY="271.0" prefHeight="54.0" prefWidth="297.0" promptText="PASSWORD" />
        <PasswordField fx:id="textFieldRepeatPassword" alignment="CENTER" layoutX="52.0" layoutY="343.0" prefHeight="54.0" prefWidth="297.0" promptText="REPEAT PASSSWORD" />
        <Button fx:id="buttonSignUpReg" layoutX="53.0" layoutY="475.0" mnemonicParsing="false" onAction="#signUp" prefHeight="41.0" prefWidth="304.0" styleClass="bigButton" text="sign up" />
        <Label fx:id="labelInfo" alignment="CENTER" contentDisplay="CENTER" layoutX="56.0" layoutY="430.0" prefHeight="29.0" prefWidth="297.0" styleClass="infoLabel" />
        <Button fx:id="buttonBack" layoutY="572.0" mnemonicParsing="false" onAction="#backToLoggIn" styleClass="smallButton" text="&lt;&lt; back" />
    </children>
    <stylesheets>
        <URL value="@style.css" />
    </stylesheets>
</AnchorPane>
